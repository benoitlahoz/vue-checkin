{
  "name": "vue-airport-devtools",
  "version": "1.0.2",
  "description": "DevTools integration for vue-airport library",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./nuxt": {
      "types": "./dist/nuxt-module.d.ts",
      "import": "./dist/nuxt-module.js"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "typecheck": "vue-tsc --noEmit"
  },
  "keywords": [
    "vue",
    "devtools",
    "vue-airport",
    "registry",
    "debugging"
  ],
  "author": "Benoit Lahoz",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/benoitlahoz/vue-airport",
    "directory": "devtools"
  },
  "peerDependencies": {
    "vue": "^3.4.0",
    "vue-airport": "workspace:*",
    "vite": "^6.0.0 || ^7.0.0",
    "@nuxt/kit": "^3.0.0"
  },
  "dependencies": {
    "@vue/devtools-api": "^7.6.4"
  },
  "devDependencies": {
    "@types/node": "^22.9.3",
    "typescript": "^5.7.2",
    "vite-plugin-dts": "^4.5.4",
    "vue-tsc": "^2.1.10"
  }
}
