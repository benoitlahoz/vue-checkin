{
  "name": "@vue-airport/plugins-validation",
  "version": "1.0.0",
  "description": "Validation plugins for vue-airport (constraints, schema, etc.)",
  "author": "Benoit Lahoz",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/benoitlahoz/vue-airport",
    "directory": "packages/plugins-validation"
  },
  "keywords": [
    "vue",
    "vue3",
    "vue-airport",
    "ioc",
    "inversion-of-control",
    "dependency-injection",
    "dependency-lookup",
    "plugins",
    "validation",
    "constraints"
  ],
  "type": "module",
  "main": "./dist/vue-airport-plugins-validation.cjs",
  "module": "./dist/vue-airport-plugins-validation.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vue-airport-plugins-validation.mjs",
      "require": "./dist/vue-airport-plugins-validation.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest run",
    "clean": "rimraf dist",
    "prebuild": "npm run clean"
  },
  "peerDependencies": {
    "vue": "^3.0.0",
    "vue-airport": "workspace:*"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "rimraf": "^6.1.0",
    "typescript": "^5.6.3",
    "vite": "^6.0.5",
    "vite-plugin-dts": "^4.3.0",
    "vue": "^3.5.0",
    "vue-tsc": "^2.1.10",
    "vitest": "^1.2.2"
  }
}
