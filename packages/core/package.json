{
  "name": "vue-airport",
  "version": "1.0.5",
  "description": "A generic check-in system with local IoC container for parent/child component registration pattern in Vue 3",
  "author": "Benoit Lahoz",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/benoitlahoz/vue-airport",
    "directory": "packages/core"
  },
  "keywords": [
    "vue",
    "vue3",
    "composable",
    "registry",
    "dependency-injection",
    "dependency-lookup",
    "check-in",
    "plugin",
    "ioc"
  ],
  "type": "module",
  "main": "./dist/vue-airport.cjs",
  "module": "./dist/vue-airport.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vue-airport.mjs",
      "require": "./dist/vue-airport.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "prepublishOnly": "cp ../../README.md ."
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "rimraf": "^6.1.0",
    "typescript": "^5.6.3",
    "vite": "^6.0.5",
    "vite-plugin-dts": "^4.3.0",
    "vue": "^3.5.0",
    "vue-tsc": "^2.1.10"
  }
}
