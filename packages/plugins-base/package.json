    "build": "vite build",
    "clean": "rimraf dist",
    "prebuild": "npm run clean"
  },
  "peerDependencies": {
    "vue": "^3.0.0",
    "vue-airport": "workspace:*"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "pinia": "^3.0.0",
    "rimraf": "^6.1.0",
    "typescript": "^5.6.3",
    "vite": "^6.0.5",
    "vite-plugin-dts": "^4.3.0",
    "vue": "^3.5.0",
    "vue-tsc": "^2.1.10"
