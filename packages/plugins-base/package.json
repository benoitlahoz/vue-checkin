{
  "name": "@vue-airport/plugins-base",
  "version": "1.0.7",
  "description": "Base plugins for vue-airport (activeItem, debounce, history)",
  "author": "Benoit Lahoz",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/benoitlahoz/vue-airport",
    "directory": "packages/plugins-base"
  },
  "keywords": [
    "vue",
    "vue3",
    "vue-airport",
    "ioc",
    "inversion-of-control",
    "dependency-injection",
    "dependency-lookup",
    "plugins",
    "active",
    "pinia",
    "debounce",
    "history"
  ],
  "type": "module",
  "main": "./dist/vue-airport-plugins.cjs",
