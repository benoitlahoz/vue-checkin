{
  "name": "@vue-airport/object-transformer",
  "version": "1.0.0",
  "description": "Object transformer Vue components with vue-airport",
  "author": "Benoit Lahoz",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/benoitlahoz/vue-airport",
    "directory": "packages/object-transformer"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./styles": "./dist/object-node.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "bench": "vitest bench --reporter=default --reporter=./benchmarks/markdown-reporter.ts",
    "bench:baseline": "node benchmarks/set-baseline.js",
    "prepublishOnly": "cp ../../README.md ."
  },
  "keywords": [
    "vue",
    "transform",
    "object",
    "vue-airport",
    "transformer"
  ],
  "peerDependencies": {
    "vue": "^3.0.0",
    "vue-airport": "workspace:*"
  },
  "dependencies": {
    "@vueuse/core": "^14.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.4.0",
    "lucide-vue-next": "^0.554.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/ui": "^2.1.8",
    "@vue/test-utils": "^2.4.6",
    "happy-dom": "^15.11.7",
    "rimraf": "^6.1.0",
    "typescript": "^5.6.3",
    "vite": "^6.0.5",
    "vite-plugin-dts": "^4.3.0",
    "vitest": "^2.1.8",
    "vue": "^3.5.0",
    "vue-tsc": "^2.1.10"
  }
}
