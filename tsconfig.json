{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "paths": {
      "@/lib/*": ["./app/lib/*"],
      "@/components/*": ["./app/components/*"],
      "#vue-airport": ["./packages/core/src"],
      "#vue-airport/*": ["./packages/core/src/*"],
      "@vue-airport/plugins-base": ["./packages/plugins-base/src"],
      "@vue-airport/plugins-base/*": ["./packages/plugins-base/src/*"],
      "@vue-airport/plugins-validation": ["./packages/plugins-validation/src"],
      "@vue-airport/plugins-validation/*": ["./packages/plugins-validation/src/*"],
      "vue-airport": ["./packages/core/src"],
      "vue-airport/*": ["./packages/core/src/*"],
      "vue-airport-devtools": ["./packages/devtools/src"],
      "vue-airport-devtools/*": ["./packages/devtools/src/*"]
    }
  }
}
